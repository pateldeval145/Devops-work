pipeline {
    agent any
tools {
jdk "java11"
}
    stages ('checkout') {
        stage ('Compile Stage') {

            steps {
                withMaven(maven : 'MAVEN_3_6_3) {
                    sh 'mvn clean compile'
                }
            }
        }

        stage ('Testing Stage') {

            steps {
                withMaven(maven : 'MAVEN_3_6_3') {
                    sh 'mvn test'
                }
            }
        }


        stage ('Deployment Stage') {
            steps {
                withMaven(maven : 'MAVEN_3_6_3') {
                    sh 'mvn deploy'
                }
            }
        }
    }
}
